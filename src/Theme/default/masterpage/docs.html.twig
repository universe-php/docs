<!DOCTYPE html>
<html lang="tr">
<head>
    <title>{% block title %}{% endblock %} Alfred</title>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Bootstrap -->
    <link rel="stylesheet" type="text/css" href="/default/assets/vendor/bootstrap/css/bootstrap.min.css" />
    <link rel="stylesheet" type="text/css" href="/default/assets/vendor/font-awesome/css/all.min.css" />
    <link rel="stylesheet" type="text/css" href="/default/assets/vendor/magnific-popup/magnific-popup.min.css" />
    <link rel="stylesheet" type="text/css" href="/default/assets/vendor/highlight.js/styles/github.css" />

    {{ themeCss('stylesheet.css') }}
    {{ themeCss('color-cyan.css') }}
</head>
<body data-spy="scroll" data-target=".idocs-navigation" data-offset="125">

<!-- Preloader -->
<div class="preloader">
    <div class="lds-ellipsis">
        <div></div>
        <div></div>
        <div></div>
        <div></div>
    </div>
</div>
<!-- Preloader End -->

<!-- Document Wrapper
=============================== -->
<div id="main-wrapper">

    {% include('inc/header.html.twig') %}

    <!-- Content
    ============================ -->
    <div id="content" role="main">

        {% include('inc/sidebar.html.twig') %}

        <!-- Docs Content
        ============================ -->
        <div class="idocs-content">
            <div class="container">
                {% block content %}{% endblock %}
            </div>
        </div>

    </div>
    <!-- Content end -->

    {% include('inc/footer.html.twig') %}

</div>
<!-- Document Wrapper end -->

<!-- Back To Top -->
<a id="back-to-top" data-toggle="tooltip" title="Back to Top" href="javascript:void(0)"><i class="fa fa-chevron-up"></i></a>








<!-- JavaScript
============================ -->
<script src="/default/assets/vendor/jquery/jquery.min.js"></script>
<script src="/default/assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
<!-- Highlight JS -->
<script src="/default/assets/vendor/highlight.js/highlight.min.js"></script>
<!-- Easing -->
<script src="/default/assets/vendor/jquery.easing/jquery.easing.min.js"></script>
<!-- Magnific Popup -->
<script src="/default/assets/vendor/magnific-popup/jquery.magnific-popup.min.js"></script>
<!-- Custom Script -->
<script src="/default/assets/js/theme.js"></script>

</body>
</html>

